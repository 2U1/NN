<script type="text/javascript">
    Event.observe(document,"dom:loaded", function() {
    document.getElementById('addBlankPageButton').style.display = "none";
    document.getElementById('addCourseLinkButton').style.display = "none";
    var autoExpandWidth = 1024; // Blackboard hides the navigation panel when the screen is at or below 760px (2012 Theme).
        try {
            if(document.getElementById('globalNavPageContentArea') && document.getElementById('navigationPane')) {
                var puller = document.getElementById('globalNavPageContentArea').querySelector("#puller");
                window.addEventListener('resize',function(event) {
                    if(window.innerWidth >= autoExpandWidth && puller.classList.contains('pullcollapsed')){
                        document.getElementById('menuPuller').click();
                        document.getElementsByClassName('chatBorder').style.left = "212px";
                    }
                });
            }
        } catch (ex) {
            console.log("JSHack(UvANavExpand): Exception Thrown: " + ex);
        }
    });
</script>

