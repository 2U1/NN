    <script>
        var course_role = "${context.CourseMembership.Role.toFieldName()}";
        var education_level = "${context.User.EducationLevel.toFieldName()}";
        var course_role_nm, education_level_nm;

        switch (course_role) {
            case "STUDENT":
                course_role_nm = "학습자";
                break;
            case "INSTRUCTOR":
                course_role_nm = "교수자";
                break;
            case "COURSE_BUILDER":
                course_role_nm = "코스생성자";
                break;
            case "GRADER":
                course_role_nm = "채점자";
                break;
            case "GUEST":
                course_role_nm = "게스트";
                break;
            case "TEACHING_ASSISTANT":
                course_role_nm = "수업조교";
                break;
            default:
                course_role_nm = "N/A";
                break;
        }

        switch (education_level) {
            case "FRESHMAN":
                education_level_nm = "1학년";
                break;
            case "SOPHOMORE":
                education_level_nm = "2학년";
                break;
            case "JUNIOR":
                education_level_nm = "3학년";
                break;
